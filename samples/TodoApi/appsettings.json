{
  "API_VERSION": "1.0",
  "SERVICE_VERSION": "0.0.1",
  "QualifiedAssemblyPattern": "NetCoreKit.Samples.*",
  "Hosts": {
    "BasePath": "/",
    "Externals": {
      "CurrentUri": "http://localhost:32501"
    }
  },
  "EfCore": {
    "Enabled": true
  },
  "RestClient": {
    "Enabled": true
  },
  "CleanArchitecture": {
    "Enabled": true
  },
  "ApiVersion": {
    "Enabled": true
  },
  "OpenApi": {
    "Enabled": true,
    "UI": {
      "Enabled": true 
    },
    "Profiler": {
      "Enabled": true
    }
  },
  "ConnectionStrings": {
    "MsSqlDb": "Server=tcp:{0},{1};Database={2};User Id={3};Password={4};",
    "MySqlDb": "server={0};port={1};uid={2};pwd={3};database={4}"
  },
  "MySqlDb": {
    "Database": "maindb",
    "UserName": "root",
    "Password": "letmein",
    "DbInfo": "5.7.14-mysql"
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug",
        "System": "Information",
        "Microsoft": "Information"
      }
    }
  },
  "AllowedHosts": "*"
}
