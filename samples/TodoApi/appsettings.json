{
	"API_VERSION": "1.0",
	"SERVICE_VERSION": "0.0.1",
	"EnableAuthN": false,
	"EfCore": {
		"FullyQualifiedPrefix": "samples.*",
		"ShortyQualifiedPrefix": "samples"
	},
  "Hosts": {
    "BasePath": "/todo/",
    "Externals": {
      "CurrentUri": "http://api.dotnetkit.local/todo"
    }
  },
	"OpenApi": {
		"Enabled": true,
		"EnabledUI": true
	},
	"Logging": {
		"IncludeScopes": false,
		"Debug": {
			"LogLevel": {
				"Default": "Warning"
			}
		},
		"Console": {
			"LogLevel": {
				"Default": "Warning"
			}
		}
	}
}
